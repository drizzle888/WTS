<?xml version="1.0"  encoding="utf-8" standalone="yes" ?>
<conf>
	<!-- 单点登陆-只能对接wcp -->
	<group describe="单点登陆" name="sso">
		<parameter name="config.sso.state">
			<val>false</val>
			<describe>是否启用单点登陆</describe>
		</parameter>
		<parameter name="config.sso.url">
			<val>http://127.0.0.1:8080/wcp</val>
			<describe>单点登系统的url，如：http://127.0.0.1:8080/wcp</describe>
		</parameter>
		<parameter name="config.sso.home.url">
			<val>http://127.0.0.1:8080/wcp/home/Pubindex.html</val>
			<describe>服务器首页</describe>
		</parameter>
		<parameter name="config.sso.title">
			<val>WCP知识库</val>
			<describe>服务端展示名称</describe>
		</parameter>
		<parameter name="config.sso.secret">
			<val>F59BD65F7EDAFB087A81D4DCA06C4910</val>
			<describe>单点登系统的安全码</describe>
		</parameter>
		<parameter name="config.sso.ologinname">
			<val>sysadmin</val>
			<describe>遠程用户登陆名</describe>
		</parameter>
		<parameter name="config.sso.opassword">
			<val>111111</val>
			<describe>远程用户登陆密码</describe>
		</parameter>
	</group>
	<!-- restful接口 -->
	<group describe="restful接口" name="restful">
		<parameter name="config.restful.state">
			<val>false</val>
			<describe>是否启用restful接口</describe>
		</parameter>
		<parameter name="config.restful.debug">
			<val>false</val>
			<describe>是否启用接口调试模式</describe>
		</parameter>
		<parameter name="config.restful.secret.key">
			<val>FB087A81D4DCA06C4910</val>
			<describe>系统通用的接口密码</describe>
		</parameter>
	</group>
	<!-- 对外RMI接口 -->
	<group describe="对外RMI接口" name="rmi">
		<parameter name="config.local.rmi.state">
			<val>false</val>
			<describe>是否启用rmi接口</describe>
		</parameter>
		<parameter name="config.local.rmi.port">
			<val>8701</val>
			<describe>对外RMI端口</describe>
		</parameter>
		<parameter name="config.local.rmi.secret.key">
			<val>1D4DCA06C49105F7EDAFB087A</val>
			<describe>对外RMI接口密码</describe>
		</parameter>
	</group>
</conf>